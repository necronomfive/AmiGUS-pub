[vamos]
# cwd=sys:ami/path
pure_ami_paths=True
hw_access=emu
cpu=68020
ram_size=10240
#8192
stack=256
quiet=True
#verbose=True

[volumes]
sys=~/Documents/dev/amigaos-vamos-env
gus=~/Documents/dev/AmiGUS/mine

[path]
auto_assign=sys:
path=sys:C,sc:c,vbcc:bin,developer:Make-4.4.1

[assigns]
developer=sys:
NDK_3.2=developer:NDK_3.2
NDK_1.3=developer:NDK_1.3
AHI4Develop=developer:AHI4/Developer

sc=developer:sc
lib=sc:lib
include=NDK_3.2:Include_H,sc:include
cxxinclude=sc:cxxinclude

vbcc=developer:vbcc
vinclude3=NDK_3.2:Include_H,vbcc:targets/m68k-amigaos/include
vlibos3=vbcc:targets/m68k-amigaos/lib
