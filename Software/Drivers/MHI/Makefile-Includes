PATH    = include

MHI_INCLUDES    = MHIDevelop:Include

all: $(LIBRARYNAME)
	copy $(MHI_INCLUDES)/libraries $(PATH)/ASM/libraries   ALL CLONE

clean:
	-@delete $(PATH) ALL
