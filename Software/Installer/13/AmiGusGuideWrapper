STACK 32768

ASSIGN AmiGUS-Manual: ""

IF EXISTS Sys:Utilities/MultiView
  RUN >NIL: Sys:Utilities/MultiView AmiGUS-Manual:AmiGUS.guide WINDOWWIDTH=666
ELSE
  IF EXISTS Sys:Utilities/AmigaGuide
    RUN >NIL: Sys:Utilities/AmigaGuide AmiGUS-Manual:AmiGUSv38.guide WINDOWWIDTH=666
  ELSE
    ECHO "Please install AmigaGuide from"
    ECHO "*e[33mhttps://aminet.net/text/hyper/aguide34.lha"
    WAIT 10
  ENDIF
ENDCLI
